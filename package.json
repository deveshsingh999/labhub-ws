{
  "name": "labhub_ws",
  "version": "0.1.0",
  "description": "labhub_ws",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "node:start": "node ./dist/index.js",
    "start": "ts-node --esm ./src/index.ts",
    "dev": "cross-env NODE_ENV=development nodemon --quiet --watch './src/**/*.ts' --exec 'node --loader ts-node/esm' ./src/index.ts",
    "prod": "cross-env NODE_ENV=production run-s -s clean build node:start",
    "prod:start": "cross-env NODE_ENV=production run-s -s node:start",
    "clean": "shx rm -rf dist",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/abraj-dew/labhub_ws.git"
  },
