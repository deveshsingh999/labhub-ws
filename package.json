  "engines": {
    "node": "^16.0.0"
  },
  "bugs": {
    "url": "https://github.com/abraj-dew/labhub_ws/issues"
  },
  "homepage": "https://github.com/abraj-dew/labhub_ws#readme",
  "dependencies": {
    "chalk": "^5.2.0",
    "dotenv": "^16.0.3"
  },
  "devDependencies": {
    "@types/node": "^18.11.15",
    "cross-env": "^7.0.3",
    "nodemon": "^2.0.20",
    "npm-run-all": "^4.1.5",
    "shx": "^0.3.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  }
